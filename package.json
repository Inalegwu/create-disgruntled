	},
	"dependencies": {
		"@effect/cli": "^0.49.7",
		"@effect/platform": "^0.70.7",
		"@effect/platform-bun": "^0.50.7",
		"effect": "^3.13.1",
		"username": "^7.0.0"
	},
	"trustedDependencies": ["@biomejs/biome", "@parcel/watcher"]
